    "X_errors = []\n",
    "y_errors = []\n",
    "z_errors = []\n",
    "\n",
    "for pred, label in zip(predictions, y_test):\n",
    "#     print(str(pred) + \" ------ \" + str(label))\n",
    "    \n",
    "    X_err = label[0] - pred[0]\n",
    "    y_err = label[1] - pred[1]\n",
    "    z_err = label[2] - pred[2]\n",
    "    \n",
    "    X_errors.append(X_err)\n",
    "    y_errors.append(y_err)\n",
    "    z_errors.append(z_err)\n",
    "    \n",
    "print(\"avg X: \" + str(np.sum(X_errors)/len(X_errors)))\n",
    "print(\"avg y: \" + str(np.sum(y_errors)/len(y_errors)))\n",
    "print(\"avg z: \" + str(np.sum(z_errors)/len(z_errors)))\n",
    "\n",
    "print(np.max(X_errors))"
